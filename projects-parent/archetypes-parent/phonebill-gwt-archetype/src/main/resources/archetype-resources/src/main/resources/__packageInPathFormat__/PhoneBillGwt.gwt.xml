#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<module rename-to="phonebill">
  <inherits name="com.google.gwt.user.User"/>
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <inherits name='edu.pdx.cs410J.Projects'/>
  <entry-point class="${package}.client.PhoneBillGwt"/>


  <!-- http://www.gwtproject.org/doc/latest/DevGuideLogging.html -->
  <inherits name="com.google.gwt.logging.Logging"/>
  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.developmentModeHandler" value="ENABLED"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>

  <set-property name="compiler.stackMode" value="emulated" />
  <set-configuration-property name="compiler.emulatedStack.recordFileNames" value="true" />
  <set-configuration-property name="compiler.emulatedStack.recordLineNumbers" value="true" />

  <servlet path="/phoneBill" class="${package}.server.PhoneBillServiceImpl"/>
</module>
