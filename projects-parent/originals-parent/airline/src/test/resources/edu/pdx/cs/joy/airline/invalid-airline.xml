<!-- A sample airline XML file -->
<!DOCTYPE airline
        PUBLIC "-//Joy of Coding at PSU//DTD Airline//EN" "airline.dtd">

<airline>
  <name>Example Airlines</name>
  <flight>
    <number>1437</number>
    <src>BJX</src>
