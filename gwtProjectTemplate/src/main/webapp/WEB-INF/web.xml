<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>pingServlet</servlet-name>
    <servlet-class>edu.pdx.cs399J.YOU.server.PingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>pingServlet</servlet-name>
    <url-pattern>/edu.pdx.cs399J.YOU.Project6/ping</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Project6.html</welcome-file>
  </welcome-file-list>
</web-app>
